# VLM Provider: "gemini" or "groq"
VLM_PROVIDER=groq

# Google AI Studio API Key (get free at https://aistudio.google.com/apikey)
GEMINI_API_KEY=your_gemini_key_here

# Gemini Model (best free-tier throughput: 15 RPM, 1000 RPD)
GEMINI_MODEL=gemini-2.5-flash-lite

# Groq API Key (get free at https://console.groq.com/keys â€” 14,400 RPD!)
GROQ_API_KEY=your_groq_key_here

# Groq Vision Model
GROQ_MODEL=meta-llama/llama-4-scout-17b-16e-instruct

# Rate limiting (used for Gemini; Groq handles its own)
RATE_LIMIT_RPM=15
RATE_LIMIT_RPD=1000

# Dataset
DATA_DIR=data
SAMPLE_SIZE=200
EVAL_SUBSET_SIZE=50
